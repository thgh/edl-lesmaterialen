{
  "name": "edl-uni-muenster",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "NODE_OPTIONS=--no-deprecation next dev -p 25077",
    "devsafe": "rm -rf .next && NODE_OPTIONS=--no-deprecation next dev -p 25077",
    "generate:importmap": "NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "types": "NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "NODE_OPTIONS=--no-deprecation next lint",
    "payload": "NODE_OPTIONS=--no-deprecation payload",
    "start": "NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "^3.66",
    "@payloadcms/next": "^3.66",
    "@payloadcms/payload-cloud": "^3.66",
    "@payloadcms/richtext-lexical": "^3.66",
    "@payloadcms/ui": "^3.66",
    "dotenv": "16.4.7",
    "graphql": "^16.8.1",
    "next": "^15.5",
    "payload": "^3.66",
    "react": "19.1.0",
    "sharp": "0.34.2",
    "swr": "^2.3.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a"
}
